private toCurrentProyecto(temp) {
  this.currentProyecto.id = temp.id;
  this.currentProyecto.estatus= temp.estatus;
  this.currentProyecto.etapa= temp.etapa;
  this.currentProyecto.mensajes = temp.mensajes ? temp.mensajes : [];
  <%_ entity.properties.forEach(property => { _%>
    <%_ if (property.multiplicidad === 'one') { _%>
  this.currentProyecto.<%= property.camelCase%> = temp.<%= property.camelCase%>;
    <%_ } else { _%>
  this.currentProyecto.<%= property.camelCase%> = temp.<%= property.camelCase%> ? temp.<%= property.camelCase%>: [];
    <%_ } _%>
  <%_ }); _%>
}

private toTempProyecto(proyecto) {
  this.tempProyecto = new Proyecto();
  this.tempProyecto.id = proyecto.id;
  this.tempProyecto.estatus= proyecto.estatus;
  this.tempProyecto.etapa= proyecto.etapa;
  this.tempProyecto.mensajes = proyecto.mensajes ? proyecto.mensajes : [];
  <%_ entity.properties.forEach(property => { _%>
    <%_ if (property.multiplicidad === 'one') { _%>
  this.tempProyecto.<%= property.camelCase%> = proyecto.<%= property.camelCase%>;
    <%_ } else { _%>
  this.tempProyecto.<%= property.camelCase%> = proyecto.<%= property.camelCase%> ? proyecto.<%= property.camelCase%>: [];
    <%_ } _%>
  <%_ }); _%>
}
